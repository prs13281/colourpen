<div class="container">
  <div class = "row">
    <div class = "offset-md-1 col-md-10">
      <h5>会員一覧</h5>
    <div class = "col-md-10">
      <table class="table table-hover table-inverse">
        <thead class="thead-light">
          <tr>
            <th></th>
            <th>会員ID</th>
            <th>名前</th>
            <th>ステータス</th>
            </tr>
        </thead>
        <tbody>
          <% @users.each do |users| %>
            <tr>
              <th></th>
              <th><%= users.id %></th>
              <th>
                <%= link_to admin_users_path(users.id) do %>
                <%= users.name %>
                <% end %>
              </th>
              <th>
                <% if users.is_deleted == true %>
                  退会
                <% else %>
                  有効
                <% end %>
              </th>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>


